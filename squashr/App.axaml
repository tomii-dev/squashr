<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:squashr"
			 xmlns:vm="using:squashr.ViewModels"
             x:Class="squashr.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

	<Application.Resources>
		<vm:CreateAccountViewModel x:Key="CreateAccountViewModel" />
		<vm:CreateLocalUserViewModel x:Key="CreateLocalUserViewModel" />
		<vm:SelectUserViewModel x:Key="SelectUserViewModel" />
		<vm:ProjectViewModel x:Key="ProjectViewModel" />
	</Application.Resources>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
</Application>
