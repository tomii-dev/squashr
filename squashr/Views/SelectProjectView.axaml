<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 FontFamily="Gidole" x:Class="squashr.Views.SelectProjectView"
			 Background="#d4e6f0" DataContext="{StaticResource SelectProjectViewModel}">
	<Panel>
		<TextBlock Text="{Binding Title}" HorizontalAlignment="Center" Margin="20" 
				   FontSize="40"/>
		<StackPanel Name="Panel" Orientation="Horizontal" HorizontalAlignment="Center">

		</StackPanel>
	</Panel>
</UserControl>